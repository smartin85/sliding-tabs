{
  "name": "sliding-tabs",
  "version": "1.0.0",
  "description": "Web Component for slidable tabs",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve"
  },
  "dependencies": {},
  "devDependencies": {
    "@stencil/core": "^0.11.0",
    "@stencil/sass": "^0.1.0",
    "@types/jest": "^23.3.1",
    "jest": "^23.4.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smartin85/sliding-tabs.git"
  },
  "author": "Smartin Nematronic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smartin85/sliding-tabs/issues"
  },
  "homepage": "https://github.com/smartin85/sliding-tabs#readme",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
